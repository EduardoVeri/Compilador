int soma(int vetor[]){
    int resp;
    output(vetor[0]);
    output(vetor[1]);
    resp = vetor[0] + vetor[1];
    return resp;
}

void main(void){
    int vetor[10];
    vetor[0] = 30;
    vetor[1] = 50;

    vetor[1+3] = 4;
    output(vetor[4]);
    vetor[2] = 6;
    output(vetor[2]);

    output(soma(vetor));
}